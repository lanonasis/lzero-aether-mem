{
  "name": "@lanonasis/shared",
  "version": "2.0.0-hackathon",
  "description": "Shared SDK, types, theme, and utilities for LanOnasis cross-platform Memory-as-a-Service",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./src/index.ts",
      "types": "./src/index.ts"
    },
    "./sdk": {
      "import": "./src/sdk/index.ts",
      "types": "./src/sdk/index.ts"
    },
    "./sdk/react-hooks": {
      "import": "./src/sdk/react-hooks.tsx",
      "types": "./src/sdk/react-hooks.tsx"
    },
    "./ai": {
      "import": "./src/ai/embeddings.ts",
      "types": "./src/ai/embeddings.ts"
    },
    "./types": {
      "import": "./src/types/index.ts",
      "types": "./src/types/index.ts"
    },
    "./theme": {
      "import": "./src/theme/index.ts",
      "types": "./src/theme/index.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "lint": "eslint . --ext ts,tsx",
    "typecheck": "tsc --noEmit",
    "test": "vitest run"
  },
  "dependencies": {
    "@lanonasis/mem-intel-sdk": "^1.1.0",
    "@tanstack/react-query": "^5.60.5",
    "@xenova/transformers": "^2.17.2",
    "zustand": "^5.0.0"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0 || 19.3.0-canary-378973b3-20251205",
    "lucide-react": "^0.400.0"
  },
  "devDependencies": {
    "@types/node": "^20.19.0",
    "@types/react": "^19.2.0",
    "typescript": "^5.6.3",
    "vitest": "^4.0.17",
    "@vitest/coverage-v8": "^4.0.17"
  }
}