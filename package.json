    "vite": "^7.3.1",
    "vitest": "^4.0.17"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "packageManager": "bun@1.3.2",
  "resolutions": {
    "esbuild": "0.27.2"
  },
