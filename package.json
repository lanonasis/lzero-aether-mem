{
  "name": "lanonasis-aether-memory",
  "version": "2.0.0-hackathon",
  "type": "module",
  "private": true,
  "description": "ARM-Optimized AI Memory Companion - Cross-platform Memory-as-a-Service with On-Device Intelligence",
  "repository": {
    "type": "git",
    "url": "https://github.com/lanonasis/aether-memory"
  },
  "author": "LanOnasis <hello@lanonasis.com>",
  "license": "MIT",
  "keywords": [
    "arm",
    "ai",
    "memory",
    "embeddings",
    "offline-first",
    "pwa",
    "vscode-extension",
    "developer-tools",
    "transformers.js",
    "maas",
    "mcp"
  ],
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "dev": "turbo run dev",
    "dev:web": "vite dev --port 5000",
    "dev:mobile": "turbo run dev --filter=@lanonasis/mobile-pwa",
    "dev:extension": "turbo run dev --filter=@lanonasis/vscode-extension",
    "dev:all": "concurrently \"bun run dev:web\" \"bun run dev:mobile\"",
    "dev:server": "tsx watch server/index.ts",
    "build": "turbo run build",
    "build:web": "vite build",
    "build:mobile": "turbo run build --filter=@lanonasis/mobile-pwa",
    "build:extension": "turbo run build --filter=@lanonasis/vscode-extension",
    "build:shared": "turbo run build --filter=@lanonasis/shared",
    "test": "turbo run test",
    "lint": "turbo run lint",
    "typecheck": "turbo run typecheck",
    "clean": "turbo run clean && rm -rf node_modules .turbo",
    "start": "NODE_ENV=production node dist/index.js"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.17",
    "@tailwindcss/vite": "^4.1.17",
    "@types/express": "^5.0.5",
    "@types/node": "^20.19.0",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "autoprefixer": "^10.4.22",
    "concurrently": "^8.2.2",
    "express": "^5.1.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.17",
    "tsx": "^4.20.6",
    "turbo": "^2.6.1",
    "typescript": "^5.6.3",
    "vite": "^7.2.4"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "packageManager": "bun@1.3.2",
  "dependencies": {
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-toast": "^1.2.15",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@tanstack/react-query": "^5.90.11",
    "@xenova/transformers": "^2.17.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "framer-motion": "^12.23.24",
    "lucide-react": "^0.555.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "tailwind-merge": "^3.4.0",
    "tw-animate-css": "^1.4.0",
    "wouter": "^3.8.0",
    "zod": "^3.25.76"
  }
}
